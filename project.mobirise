      "settings": {
        "main": true,
        "title": "Formação Engenheiro de Rodovias",
        "meta_descr": "",
        "header_custom": "<script>\ndocument.addEventListener(\"DOMContentLoaded\", function() {\n    // Esconde todos os campos que não são email e phone\n    const formFields = document.querySelectorAll('.form-group');\n    formFields.forEach(field => {\n        const input = field.querySelector('input');\n        if (input && input.name !== 'email' && input.name !== 'phone') {\n            field.style.display = 'none';\n        }\n    });\n\n    // Função para obter parâmetros da URL\n    function getQueryParam(param) {\n        const urlParams = new URLSearchParams(window.location.search);\n        return urlParams.get(param) || '';\n    }\n\n    // Função para obter o endereço IP\n    async function getUserIP() {\n        try {\n            const response = await fetch('https://api.ipify.org?format=json');\n            const data = await response.json();\n            return data.ip;\n        } catch (error) {\n            console.error('Erro ao obter o IP:', error);\n            return '';\n        }\n    }\n\n    // Função para separar o DDD do telefone\n    function splitPhoneNumber(phone) {\n        const cleanPhone = phone.replace(/\\D/g, ''); // Remove todos os caracteres não numéricos\n        const ddd = cleanPhone.slice(0, 2); // Os dois primeiros números são o DDD\n        const phoneNumber = cleanPhone.slice(2); // O restante é o número do telefone\n        return { ddd, phoneNumber };\n    }\n\n    // Adiciona evento ao formulário\n    const form = document.querySelector(\"form.mbr-form\");\n    if (form) {\n        form.addEventListener(\"submit\", async function(event) {\n            // Coleta valores de email e phone\n            const email = form.querySelector(\"input[name='email']\").value;\n            const phone = form.querySelector(\"input[name='phone']\").value;\n\n            // Separa o DDD e o número do telefone\n            const { ddd, phoneNumber } = splitPhoneNumber(phone);\n\n            // Coleta valores adicionais como utm, ip, url, e language\n            const utm_source = getQueryParam('utm_source');\n            const utm_medium = getQueryParam('utm_medium');\n            const utm_term = getQueryParam('utm_term');\n            const utm_campaign = getQueryParam('utm_campaign');\n            const utm_content = getQueryParam('utm_content');\n            const ip = await getUserIP(); // Obtém o IP via API\n            const url = window.location.href;\n            const language = navigator.language;\n\n            // Verifica se os campos obrigatórios estão preenchidos\n            if (email && phone) {\n                // Redireciona para o checkout da Hotmart com email, DDD e número do telefone via webhook\n                const hotmartUrl = `https://pay.hotmart.com/P94742783C?off=565q5ols&email=${encodeURIComponent(email)}&phoneac=${encodeURIComponent(ddd)}&phonenumber=${encodeURIComponent(phoneNumber)}`;\n\n                // Cria um novo FormData e adiciona campos UTM e outros dados ao formulário\n                const formData = new FormData(form);\n                formData.append('utm_source', utm_source);\n                formData.append('utm_medium', utm_medium);\n                formData.append('utm_term', utm_term);\n                formData.append('utm_campaign', utm_campaign);\n                formData.append('utm_content', utm_content);\n                formData.append('ip', ip);\n                formData.append('url', url);\n                formData.append('language', language);\n\n                // Envia dados do formulário via AJAX\n                fetch(form.action, {\n                    method: 'POST',\n                    body: formData,\n                    headers: {\n                        'Accept': 'application/json'\n                    }\n                })\n                .then(response => {\n                    if (response.ok) {\n                        // Redireciona após o envio bem-sucedido\n                        window.location.href = hotmartUrl;\n                    } else {\n                        alert(\"Houve um problema ao enviar o formulário. Tente novamente.\");\n                    }\n                })\n                .catch(error => {\n                    console.error(\"Erro ao enviar o formulário:\", error);\n                    alert(\"Houve um problema ao enviar o formulário. Tente novamente.\");\n                });\n\n                // Previne o envio padrão do formulário\n                event.preventDefault();\n            } else {\n                alert(\"Por favor, preencha todos os campos obrigatórios.\");\n                event.preventDefault(); // Impede o envio se os campos não estão preenchidos\n            }\n        });\n    } else {\n        console.error(\"Formulário não encontrado.\");\n    }\n});\n</script>",
        "footer_custom": "",
